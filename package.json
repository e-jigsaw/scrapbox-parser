{
  "name": "@progfay/scrapbox-parser",
  "version": "4.0.0",
  "description": "parse Scrapbox notation to JavaScript Object",
  "files": [
    "lib",
    "esm",
    "umd"
  ],
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./lib/index.d.ts",
  "unpkg": "./umd/scrapbox-parser.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd",
    "prebuild:cjs": "rm -rf lib",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prebuild:esm": "rm -rf esm",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "prebuild:umd": "rm -rf umd",
    "build:umd": "webpack",
    "prepare": "npm run build",
    "test": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "lint": "eslint -c ./.eslintrc.js .",
    "lint:fix": "eslint --fix -c ./.eslintrc.js ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/progfay/scrapbox-parser.git"
  },
  "keywords": [
    "scrapbox",
    "parser"
  ],
  "author": "progfay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/progfay/scrapbox-parser/issues"
  },
  "homepage": "https://github.com/progfay/scrapbox-parser#readme",
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@types/core-js": "2.5.3",
    "@types/jest": "25.2.3",
    "@types/node": "13.13.9",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "babel-loader": "8.1.0",
    "eslint": "7.1.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.13.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "jest": "26.0.1",
    "jest-snapshot": "26.0.1",
    "ts-jest": "26.0.0",
    "ts-loader": "7.0.4",
    "typescript": "3.9.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "dependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
